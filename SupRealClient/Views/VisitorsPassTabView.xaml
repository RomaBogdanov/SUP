<mah:MetroWindow x:Class="SupRealClient.Views.VisitorsPassTabView"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:mah="http://metro.mahapps.com/winfx/xaml/controls"
        mc:Ignorable="d"
        Title="Пропуска" Height="350" Width="700"
        ResizeMode="NoResize"
        WindowStartupLocation="CenterScreen"
        KeyDown="VisitorsPassTabView_OnKeyDown">
    <Grid>
        <DataGrid AutoGenerateColumns="False">
            <DataGrid.Columns>
                <DataGridTextColumn Header="Пропуск"/>
                <DataGridTextColumn Header="Когда выдан"/>
                <DataGridTextColumn Header="Срок по"/>
                <DataGridTextColumn Header="Изменён"/>
                <DataGridTextColumn Header="Оператор"/>
                <DataGridTextColumn Header="№ заявки"/>
                <DataGridTextColumn Header="Дополнительно" Width="*"/>
            </DataGrid.Columns>
        </DataGrid>
    </Grid>
</mah:MetroWindow>
